{
  "name": "dtls",
  "version": "0.0.0",
  "description": "Datagram Transport Layer Security protocol in pure js / WebAssembly.",
  "main": "index.js",
  "dependencies": {
    "aggregate-error": "^1.0.0",
    "date2buffer": "^1.0.0",
    "debug": "^3.1.0",
    "denque": "^1.2.2",
    "is-dtls": "^1.0.0",
    "is-plain-object": "^2.0.4",
    "unicast": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "concat-stream": "^1.6.0",
    "xo": "^0.18.2"
  },
  "scripts": {
    "test": "ava -T 3000",
    "start": "node example.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodertc/dtls.git"
  },
  "keywords": [
    "dtls",
    "tls",
    "ssl",
    "crypto",
    "udp",
    "dgram",
    "wasm",
    "webassembly",
    "gnutls",
    "gnu"
  ],
  "author": "Dmitry Tsvettsikh <me@reklatsmasters.com>",
  "bugs": {
    "url": "https://github.com/nodertc/dtls/issues"
  },
  "homepage": "https://github.com/nodertc/dtls#readme",
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "ignores": [
      "**/test/**"
    ],
    "globals": [
      "WebAssembly"
    ]
  }
}
